        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/vcpkg/**": true,
        "build.x86.debug": true,
        "build.x86.release": true,
        "build.x64.debug": true,
